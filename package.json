{
  "name": "Legislated",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "ios": "yarn ios:run:dev",
    "ios:run:dev": "react-native run-ios --scheme Legislated",
    "ios:log": "react-native log-ios",
    "lint": "standard | snazzy",
    "lint:fix": "standard --fix | snazzy"
  },
  "dependencies": {
    "react": "16.0.0-alpha.6",
    "react-native": "^0.43.2",
    "react-relay": "^0.10.0",
    "react-router-native": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "babel-relay-plugin": "0.10.0",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "jest": "19.0.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "flow-typed"
    ],
    "globals": [
      "React$Element"
    ]
  }
}
